<network>
  <name>foreman-bootstrap</name>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <bridge name='pxebr' stp='on' delay='0'/>
  <dns>
    <host ip='xxxIPxxx'>
      <hostname>kickstart</hostname>
    </host>
  </dns>
  <ip address='xxxIPxxx' netmask='xxxNETMASKxxx'>
    <tftp root='/var/lib/tftpboot'/>
    <dhcp>
      <range start='xxxRANGE_STARTxxx' end='xxxRANGE_ENDxxx'/>
      <bootp file='pxelinux.0'/>
    </dhcp>
  </ip>
</network>

