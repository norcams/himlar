apiVersion: projectcalico.org/v3
kind: NetworkPolicy
metadata:
  name: nrec-deny-management
  namespace: openstack
spec:
  order: 100
  types:
    - Egress
  egress:
  - action: Deny
    destination:
      nets:
      - '172.16.0.0/12'
  - action: Deny
    destination:
      nets:
      - 'fd00::/8'
  selector: "projectcalico.org/orchestrator == 'openstack'"
